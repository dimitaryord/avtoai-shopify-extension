{
    "ProductInformation": {
        "title": "Product Information",
        "description": "Knowing more about your brand products and customer will help us create AVTO much better than it's default version. Try to view from this point, as you are trying to explain to a friend what are you are selling, why are people buying it, what separates you from others, who are you selling too. ",
        "components": [
            {
                "type": "input",
                "id": "storeName",
                "label": "What is the name of your Shopify Store?",
                "options": [
                    "Default"
                ],
                "rules": "required",
                "errorName": "Store Name"
            },
            {
                "type": "input",
                "id": "brandInfo",
                "label": "Brand Information",
                "placeholder": "Describe your brand here",
                "multiline": 4,
                "rules": "required",
                "errorName": "Brand Information"
            }
        ]
    },
    "Brand Category": {
        "title": "Brand Category",
        "description": "What is your industry?",
        "components": [
            {
                "type": "choice",
                "id": "industry",
                "title": "In which industry is your Shopify store?",
                "choices": [
                    {
                        "label": "Lifestyle & Wellness",
                        "value": "lifestyle"
                    },
                    {
                        "label": "Fashion & Personal Accessories",
                        "value": "fashion"
                    },
                    {
                        "label": "Home & Living",
                        "value": "home"
                    },
                    {
                        "label": "Sports & Recreation",
                        "value": "sports"
                    },
                    {
                        "label": "Toys & Games",
                        "value": "toys"
                    }
                ]
            }
        ]
    },
    "Other Information": {
        "title": "Other Information",
        "description": "Just if you want to tell us something more about your brand, you can do it here, if not just click next.",
        "components": [
            {
                "type": "input",
                "id": "otherInfo",
                "label": "Something you think we need to know?",
                "placeholder": "Something specific about my company is...",
                "multiline": 4,
                "rules": "required",
                "errorName": "Other Information"
            }
        ]
    }
}